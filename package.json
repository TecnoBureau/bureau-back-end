{
  "name": "bureau-back-end",
  "version": "0.0.0",
  "description": "Backend do projeto Bureau Una Barreiro.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "build": "node src/index.js",
    "dev": "node --env-file=.env --watch src/index.js",
    "test": "node --env-file=.env --test test/",
    "test:coverage": "node --env-file=.env --experimental-test-coverage --test test/",
    "test:debugger": "node --env-file=.env --inspect --watch --test test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TecnoBureau/bureau-back-end.git"
  },
  "keywords": [
    "bureau"
  ],
