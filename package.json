  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TecnoBureau/bureau-back-end/issues"
  },
  "homepage": "https://github.com/TecnoBureau/bureau-back-end#readme",
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "env-var": "^7.5.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^8.4.4",
    "node-cron": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
